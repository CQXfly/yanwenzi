# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'


use_frameworks!
def mypods
    pod 'Alamofire'
    pod 'SnapKit'
    pod 'RxSwift'
end
    
target 'yanwenzi' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks

    # Pods for yanwenzi
    mypods
 	
  target 'yanwenziTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'yanwenziUITests' do
    inherit! :search_paths
    # Pods for testing
  end
end

target 'YANKit' do
    mypods
end

target 'yanInput' do
    mypods
end

target 'yanToday' do
    mypods
end



